{{#each userBlogs as |userBlog|}}
<div class="row">
  <div class="col-md-3"></div>
  <div class="d-flex justify-content-start mx-3 mt-3 col-md-6" style="border: solid 0.5px rgb(255, 153, 0);">
    <div class="mt-2">
      <a href="/blogs/admin/{{userBlog.id}}"><h2 style="color: rgba(104, 55, 182, 0.817);">{{userBlog.title}}</h2></a>
      <p class="mb-2">Created on {{format_date userBlog.date_created}}</p>
    </div>
  </div>
  <div class="col-md-3"></div>
</div>
{{/each}}
